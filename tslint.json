{
    "extends": [
        "tslint:latest"
    ],
    "rulesDirectory": [
        "./rules"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "object-literal-smart-keys": true,
        "member-access": [
            false
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "always"
            }
        ]
    }
}